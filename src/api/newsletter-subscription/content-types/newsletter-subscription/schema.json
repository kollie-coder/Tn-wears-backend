{
  "kind": "collectionType",
  "collectionName": "newsletter_subscriptions",
  "info": {
    "singularName": "newsletter-subscription",
    "pluralName": "newsletter-subscriptions",
    "displayName": "Newsletter-subscription"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true
    },
    "subscribed": {
      "type": "boolean",
      "default": true
    },
    "subscribedAt": {
      "type": "datetime"
    },
    "unsubscribedAt": {
      "type": "datetime"
    },
    "source": {
      "type": "string",
      "default": "website"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "newsletter_subscription"
    }
  }
}
